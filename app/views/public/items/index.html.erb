<h1>Public::Items#index</h1>
<p>Find me in app/views/public/items/index.html.erb</p>


<div class="row text-left">
  <h3 class="font-weight-normal mt-3">商品一覧</h3>
</div>
<div class="">
  <% @items.each do |item| %>
  <div class="one-post">
    <%= image_tag item.get_image, size: "150x150" %>
    <p class="caption"><%= link_to item.name, item_path(item.id) %></p>
    <p class="caption"><<%= item.price %></p>
    <% end %>
  </div>
</div>
<%= paginate @items %>
</div>