<h1>Public::Items#index</h1>
<p>Find me in app/views/public/items/index.html.erb</p>


<div>
<table>
  <thead>
       <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
  </thead>
  <tbody>
    <% @items.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td><%= link_to item.name, item_path(item.id) %></td>
      <td><%= item.price %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @items %>
</div>